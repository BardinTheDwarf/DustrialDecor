{
  "multipart": [
    {   "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_post_ends" }},
    {   "when": { "north": false, "east": false, "south": false, "west": false },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_post" }
    },
    {   "when": { "north": true, "east": false, "south": false, "west": false },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_cap" }
    },
    {   "when": { "north": false, "east": true, "south": false, "west": false },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_cap", "y": 90 }
    },
    {   "when": { "north": false, "east": false, "south": true, "west": false },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_cap_alt" }
    },
    {   "when": { "north": false, "east": false, "south": false, "west": true },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_cap_alt", "y": 90 }
    },
    {   "when": { "north": true },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "dustrial_decor:block/cast_iron_balustrade_side_alt", "y": 90 }
    }
  ]
}
